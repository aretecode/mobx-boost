{
  "version": "2.0.10",
  "compilerOptions": {
    "target": "es5",
    "outDir": "lib",
    "sourceMap": false,
    "declaration": true,
    "module": "commonjs",
    "removeComments": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "lib": ["es6"]
  },
  "include": ["src/mobx-boost.ts"],
  "exclude": ["node_modules"]
}
